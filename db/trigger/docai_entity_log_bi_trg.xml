<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:n0="http://www.oracle.com/xml/ns/dbchangelog-ext" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog 
	http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
	<changeSet id="trigger-docai_entity_log_bi_trg" author="VAI" failOnError="true" runOnChange="true">
		<n0:createOracleTrigger objectName="DOCAI_ENTITY_LOG_BI_TRG" objectType="trigger" ownerName="VAI">
			<n0:source><![CDATA[CREATE OR REPLACE EDITIONABLE TRIGGER "DOCAI_ENTITY_LOG_BI_TRG" 
BEFORE INSERT
ON DOCAI_ENTITY_LOG
FOR EACH ROW
BEGIN
  :new.crt_ts := SYSTIMESTAMP;
  :new.crt_by := NVL(v('APP_USER'), USER);
END;
]]>			</n0:source>
		</n0:createOracleTrigger>
	</changeSet>
</databaseChangeLog>
